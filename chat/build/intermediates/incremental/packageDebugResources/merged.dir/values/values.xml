    <declare-styleable name="QiscusAudioRecorderView">
        <attr format="color|reference" name="qrecord_cancelSrc"/>
        <attr format="color|reference" name="qrecord_stopSrc"/>
    </declare-styleable>
    <declare-styleable name="QiscusCircleProgress">
        <attr format="integer" name="qcircle_progress"/>
        <attr format="integer" name="qcircle_max"/>
        <attr format="color" name="qcircle_unfinished_color"/>
        <attr format="color" name="qcircle_finished_color"/>
        <attr format="dimension" name="qcircle_text_size"/>
        <attr format="color" name="qcircle_text_color"/>
        <attr format="string" name="qcircle_prefix_text"/>
        <attr format="string" name="qcircle_suffix_text"/>
    </declare-styleable>
    <declare-styleable name="QiscusCircularImageView">
        <attr format="dimension" name="qciv_border_width"/>
        <attr format="color" name="qciv_border_color"/>
        <attr format="boolean" name="qciv_border_overlay"/>
        <attr format="color" name="qciv_fill_color"/>
    </declare-styleable>
    <declare-styleable name="QiscusFlowLayout">
        <attr name="android:orientation"/>
        <attr format="enum" name="layoutDirection">
            <enum name="ltr" value="0"/>
            <enum name="rtl" value="1"/>
        </attr>
        <attr format="boolean" name="debugDraw"/>
        <attr format="float" name="weightDefault"/>
        <attr name="android:gravity"/>
    </declare-styleable>
    <declare-styleable name="QiscusFlowLayout_LayoutParams">
        <attr format="boolean" name="layout_newLine"/>
        <attr format="float" name="layout_weight"/>
        <attr name="android:layout_gravity"/>
    </declare-styleable>
    <declare-styleable name="QiscusLinkPreviewView">
        <attr format="color|reference" name="qpreview_titleColor"/>
        <attr format="color|reference" name="qpreview_descColor"/>
    </declare-styleable>
